{
	"name": "tutorowl",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"dev": "npx mix",
		"watch": "npx mix watch",
		"prod": "npx mix --production",
		"make-pot": "wp i18n make-pot --exclude='node_modules, vendor' . languages/tutorowl.pot",
		"bundle": "wp dist-archive . . && mv tutor-owl.zip tutor-owl-$(grep 'Version:' style.css | awk '{print $3}').zip && mv tutor-owl-* ../",
		"build": "npm run make-pot && npm run prod && npm run bundle"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"description": "",
	"devDependencies": {
		"dir-archiver": "^2.1.0",
		"laravel-mix": "^6.0.49",
		"resolve-url-loader": "^5.0.0",
		"sass": "^1.71.1",
		"sass-loader": "^12.6.0"
	},
	"dependencies": {}
}